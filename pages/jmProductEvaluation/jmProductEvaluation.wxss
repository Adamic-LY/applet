/* pages/jmProductEvaluation/jmProductEvaluation.wxss */
.jmProductEvaluationNav{
  width: 700rpx;
  margin: 20rpx 0 20rpx 30rpx;
  display: flex;
}
.jmProductEvaluationNavItem{
  width: 140rpx;
  text-align: center
}
.evaluationDetails{
  width: 690rpx;
  margin: 20rpx auto;
 
}
.companyDetails{
  display: flex
}
.companyImage image{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%
}
.companyName,.evaluationTime{
  font-size: 30rpx;
  color: grey
}
.companyDetailsTxt{
  margin-left: 30rpx;
}
.companyDetailsImageBox{
  display: flex;
  flex-wrap: wrap;
  /* justify-content: space-between */
}
.companyDetailsImage{
  width: 200rpx;
  height: 200rpx;
  margin-left: 20rpx;
  margin-bottom: 20rpx
}
.companyDetailsImage image{
  width: 200rpx;
  height: 200rpx;
}
.Commentsonthethumbup{
  display: flex;
  justify-content: flex-end;
}
.CommentsonthethumbupTxt{
  font-size: 25rpx;
  color: grey;
  margin-left: 40rpx;
  line-height: 25rpx;
}
.jmProductEvaluationTarbar{
  width: 750rpx;
  position: fixed;
  top: 300rpx;
  z-index: 1000
}